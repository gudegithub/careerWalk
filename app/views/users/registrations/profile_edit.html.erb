<%= form_for current_user, url: {action: 'profile_update'} do |f| %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :プロフィール画像 %><br/>
    <%= f.file_field :image %>
  </div>

  <div class="actions">
    <%= f.submit "Update" %>
  </div>
<% end %>
