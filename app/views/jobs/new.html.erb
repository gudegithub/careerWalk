<div class="container">
    <%= form_for @job do |form|%>
    <div class="form-group">
      <%= form.label :name  %>		
      <%= form.text_field :name, class: "form-control" %>
    </div>
    <div class="form-group">
      <%= form.label :publish_start_at  %>	    
      <%= form.text_field :publish_start_at, class: "form-control"%>
    </div>
    <%= form.submit "送信" , class: "btn btn-primary"  %>
  <% end %>
</div>

